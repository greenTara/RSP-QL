        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </DatatypeProperty>
    


    <!-- http://streamreasoning.org/timeVocab#startsAtTime -->

    <DatatypeProperty rdf:about="http://streamreasoning.org/timeVocab#startsAtTime">
